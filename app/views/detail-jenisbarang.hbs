{{!< layout }}
<div class="row mt">
          		<div class="col-lg-12">
                  <div class="form-panel">
                  	  <h4 class="mb"><i class="fa fa-angle-right"></i> Form Elements</h4>
                      <hr>
                      <form class="form-horizontal style-form">

                          <div class="form-group">
                              <div class="col-lg-12">
                                  <div class="row centered">
                                      <img src="/uploads/{{jenisbarang.image}}" class="img-circle" width="240">   
                                   </div>
                              </div>
                          </div>

                          <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">ID jenis barang</label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.id}}</p>
                              </div>
                          </div>

                          <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">Nama jenis barang</label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.nama}}</p>
                              </div>
                          </div>

                          <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">Jumlah </label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.jumlah}}</p>
                              </div>
                          </div>
                          <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">Status</label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.status}}</p>
                              </div>
                          </div>
                           <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">Kategori</label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.kategori}}</p>
                              </div>
                          </div>
                          <div class="form-group">
                              <label class="col-lg-2 col-sm-2 control-label">Deskripsi</label>
                              <div class="col-lg-10">
                                  <p class="form-control-static">{{jenisbarang.deskripsi}}</p>
                              </div>
                          </div>
                      </form>
                  </div>
          		</div><!-- col-lg-12-->      	
          	</div>